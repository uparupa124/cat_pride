<h3><%= @user.user_name %>様の会員情報編集画面</h3>
  <%= form_with model: @user, url: admin_user_path, local: true do |f| %>
   <table>
     <tbody>
       <tr>
         <td><%= f.label :"名前" %></td>
         <td><%= f.text_field :full_name %></td>
       </tr>
         <tr>
           <td><%= f.label :"メールアドレス" %></td>
           <td><%= f.text_field :email %></td>
         </tr>
           <tr>
             <td><%= f.label :"電話番号" %></td>
             <td><%= f.text_field :telephone_number %></td>
           </tr>
     </tbody>
   </table>
    <%= f.submit "変更する" %>
    <%= link_to "戻る", admin_user_path(@user.id) %>
  <% end %>